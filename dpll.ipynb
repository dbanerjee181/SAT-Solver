        "          for neighbor in self.graph[current_node]['sink']:\n",
        "            if neighbor not in visited:\n",
        "              visited.add(neighbor)\n",
        "              queue.append(neighbor)\n",
        "        return visited\n",
        "\n",
        "    def traverse_source(self, start_node):\n",
        "        visited = set()\n",
